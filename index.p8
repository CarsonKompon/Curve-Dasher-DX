--particles

function parts_init()
	parts = {}
end

function spawn_explosion(_x,_y,am,size,c)
	for i=1,am do
		add(parts,{
		siz=1+rnd(size-1),dec=0.01,fric=0.001,
		x=_x,y=_y,col=c,hs=rnd(4)-2,vs=rnd(4)-2
		})
	end
end

function parts_update()
	for p in all(parts) do
		p.hs -= p.fric*sgn(p.hs)
		p.vs -= p.fric*sgn(p.vs)
		p.x += p.hs
		p.y += p.vs
		p.siz -= p.dec
		if p.siz <= 0 then
			del(parts,p)
		end
	end
end

function parts_draw()
	for p in all(parts) do
		circfill(p.x,p.y,p.siz,p.col)
	end
end
