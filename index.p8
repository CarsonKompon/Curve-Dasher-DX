--game state

function game_init()
	gamestate=1
	player_init()
	parts_init()
	enemies = {}
	score=0
	timer=0
	spawntime=180
	maxenemies=10
	fric=0.25
	shake=0
	camx=0
	camy=0
	goy=-128
	gop=false
	combofx={}
	
	if mute then music(-1)
	else music(0) end
end

function game_update()
	if combotimer>0 then
		combotimer-=1
		if(combotimer==0)combo=0
	end
	shake = lerp(shake,0,0.125)
	if ply.dead then
		camx = lerp(camx,ply.x-64,0.0125)
		camy = lerp(camy,ply.y-64,0.0125)
		goy = lerp(goy,0,0.05)
		if btnp(‚¨ÜÔ∏è) or btnp(‚¨ÖÔ∏è) then
			gop=false
			sfx(11,2)
		elseif btnp(‚¨áÔ∏è) or btnp(‚û°Ô∏è) then
			gop=true
			sfx(11,2)
		end
		if btnp(üÖæÔ∏è) then
			if gop then
				menu_init()
			else
				game_init()
			end
		end
	end
	camera(camx+rnd(shake*2)-shake,
	camy+rnd(shake*2)-shake)
	timer+=1
	--player update
	player_update()
	--enemy spawner
	if timer%flr(spawntime)==0
	and not ply.dead
	and #enemies < maxenemies then
		local _typ = 3
		if score >= 10 and flr(rnd(10)) == 1 then
			_typ = 4
		end
		add(enemies,{
		x=rnd(120)+4,y=rnd(120)+4,
		hs=0,vs=0,siz=0,typ=_typ
		})
	end
	--enemy update
	for e in all(enemies) do
		e.siz = lerp(e.siz,e.typ,0.05)
		--collision
		local _colsiz = max(ply.spd*2,e.typ-1)
		if abs(e.x-ply.x)<_colsiz
		and abs(e.y-ply.y)<_colsiz
		and ceil(e.siz*8)/8 == e.typ then
			if flr(ply.spd) == 1 then
				if not ply.dead then
					sfx(10,3)
					sfx(18,2)
					shake = 20
				end
				music(-1)
				ply.dead = true
				spawn_explosion(ply.x,ply.y,2,3,2)
			else
				--destroy ball
				combo+=1
				if combo>1 then
					ply.spd+=1
					sfx(14+min(combo,4)-2,2)
					combo_fx()
				end
				combotimer=140
				score+=combo
				if(spawntime>30) spawntime -= 0.01
				spawn_explosion(e.x,e.y,8+rnd(6),3,2)
				del(enemies,e)
				shake = 10
				sfx(10,3)
			end
		end
		--movement
		if abs(e.hs) > fric then
			e.hs -= fric*sgn(e.hs)
		else e.hs = 0 end
		if abs(e.vs) > fric then
			e.vs -= fric*sgn(e.vs)
		else e.hs = 0 end
		e.x += e.hs
		e.y += e.vs
	end
	parts_update()
	combo_fx_update()
end

function game_draw()
	palt(13,true)
	palt(0,false)
	for i=1,300 do
		circfill(camx+rnd(128),camy+rnd(128),
		rnd(2),rnd(2))
	end
	
	parts_draw()
	
	if not ply.dead then
		for e in all(enemies) do
			circfill(e.x,e.y,e.siz,8)
		end
		
		player_draw()
		combo_fx_draw()
	end
	
	--draw ui
	if not ply.dead then
		for i=-1,1 do
			for j=-1,1 do
				print(score,2+i,2+j,0)
			end
		end
		print(score,2,2,7)
	else
		spr(0,camx+21,camy+goy+10,11,8)
		local _txts = "score: " .. score
		local _txt1 = "retry"
		local _txt2 = "main menu"
		for i=-1,1 do
			for j=-1,1 do
				print(_txts,camx+64-(#_txts*2)+i,
				camy+goy+78+j,0)
				print(_txt1,camx+64-(#_txt1*2)+i,
				camy+goy+98+j,0)
				print(_txt2,camx+64-(#_txt2*2)+i,
				camy+goy+108+j,0)
			end
		end
		print(_txts,camx+64-(#_txts*2),
		camy+goy+78,7)
		if not gop then color(7)
		else color(13) end
		print(_txt1,camx+64-(#_txt1*2),camy+goy+98)
		if gop then color(7)
		else color(13) end
		print(_txt2,camx+64-(#_txt2*2),camy+goy+108)
	end
end

function combo_fx()
	add(combofx,{
	x=ply.x-rnd(6),
	y=ply.y-rnd(3),
	txt="X"..combo,
	spd=1,
	timer=0
	})
end

function combo_fx_update()
	for i in all(combofx) do
		i.timer+=1
		i.spd-=0.05
		i.y-=max(i.spd,0)
		if(i.timer >= 120) del(combofx,i)
	end
end

function combo_fx_draw()
	for i in all(combofx) do
		for j=-1,1 do
			for k=-1,1 do
				print(i.txt,i.x+j,i.y+k,0)
			end
		end
		print(i.txt,i.x,i.y,7)
	end
end
