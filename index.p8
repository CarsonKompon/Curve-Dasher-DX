--useful functions

--screen wrapping
function wrap(i)
	if(i>128) return i-128
	if(i<0) return i+128
	return i
end

--lengthdir_x
function lenx(len,dir)
	return cos(dir)*len
end

--lengthdir_y
function leny(len,dir)
	return -sin(dir)*len
end

--linear interpolation
function lerp(pos,tar,perc)
 return pos+((tar-pos)*perc)
end


