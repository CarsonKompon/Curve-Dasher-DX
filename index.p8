--player stuff

function player_init()
	ply = {
	x=63,y=63,spd=1,dir=0,
	turn=0.01,cooldown=0,dead=false
	}
	combo=0
	combotimer=0
end

function player_update()
	if not ply.dead then
		--speed lerping
		ply.spd = lerp(ply.spd,1,0.02)
		--steering
		if btn(‚¨ÖÔ∏è) then
			ply.dir -= ply.turn
		elseif btn(‚û°Ô∏è) then
			ply.dir += ply.turn
		end
		--cooldown
		if flr(ply.spd) == 1
		and ply.cooldown > 0 then
			ply.cooldown-=1
			if(ply.cooldown == 0) sfx(9)
		end
		--dash
		if btnp(üÖæÔ∏è) and ply.cooldown == 0 then
			ply.spd = 7
			ply.cooldown = 120
			sfx(8)
		end
		--move player
		ply.x = wrap(ply.x+lenx(ply.spd,ply.dir))
		ply.y = wrap(ply.y+leny(ply.spd,ply.dir))
	end
end

function player_draw()
	if not ply.dead then
		circfill(ply.x,ply.y,3,max(3,1+ply.spd*2))
	end
end
