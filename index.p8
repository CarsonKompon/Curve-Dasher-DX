--main menu

function menu_init()
	gamestate=0
	camera(0,0)
	logoy=-128
	menu=1
	page=1
end

function menu_update()
	camera(0,0)
	logoy=lerp(logoy,0,0.125)
	
	if btnp(‚¨ÜÔ∏è) or btnp(‚¨ÖÔ∏è) then
		menu-=1
		if(menu<1)menu=1
		sfx(11,2)
	elseif btnp(‚¨áÔ∏è) or btnp(‚û°Ô∏è) then
		menu+=1
		if(menu>2) menu=2
		sfx(11,2)
	end
	if btnp(üÖæÔ∏è) then
		sfx(12)
		if page==1 then
			if menu==1 then
				game_init()
			else
				page=2
				menu=1
			end
		else
			if menu==1 then
				mute = not mute
			else
				page=1
				menu=1
			end
		end
	end
end

function menu_draw()
	palt(13,true)
	palt(0,false)
	for i=1,300 do
		circfill(rnd(128),rnd(128),rnd(2),rnd(2))
	end
	
	spr(128,14,logoy+8,13,8)
	
	local _txt1 = "start game"
	local _txt2 = "options"
	if page == 2 then
		if mute then
			_txt1 = "music: off"
		else
			_txt1 = "music: on"
		end
		_txt2 = "back"
	end
	for i=-1,1 do
		for j=-1,1 do
			print(_txt1,64-(#_txt1*2)+i,-logoy+98+j,0)
			print(_txt2,64-(#_txt2*2)+i,-logoy+108+j,0)
		end
	end
	
	if menu==1 then color(7)
	else color(13) end
	print(_txt1,64-(#_txt1*2),-logoy+98)
	if menu==2 then color(7)
	else color(13) end
	print(_txt2,64-(#_txt2*2),-logoy+108)
end
